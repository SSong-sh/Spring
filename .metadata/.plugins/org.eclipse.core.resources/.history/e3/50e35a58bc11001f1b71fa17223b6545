package com.test.begin.aop.test;

public class Main {
	public static void main(String[] args) {
		
		//주 업무 객체 > MemoImpl
		MemoImpl memo = new MemoImpl();
		
		//글쓰기
		memo.add("스프링 작업 중입니다.");
		
		//글읽기
		try {
			
			String content = memo.read(5);
			System.out.println(content);
			
		} catch (Exception e) {
			
			// TODO Auto-generated catch block
			e.printStackTrace();
			
		}
		
		//글수정
		boolean result = memo.edit(15, "수정할 내용");
		
		//글삭제
		result = memo.del(15);
		
	}
}
